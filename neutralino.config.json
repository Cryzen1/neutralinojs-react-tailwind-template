{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "filesystem.readDirectory"],
  "modes": {
    "window": {
      "title": "neutralinojs-react-tailwind-template",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/react-src/public/vite.svg"
    }
  },
  "cli": {
    "binaryName": "neutralinojs-react-tailwind-template",
    "resourcesPath": "/react-src/dist/",
    "binaryVersion": "5.1.0",
    "clientVersion": "5.1.0",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/react-src/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}
